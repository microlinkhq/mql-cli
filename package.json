{
  "name": "@microlink/mql-cli",
  "description": "CLI for interacting with Microlink Query Language",
  "homepage": "https://nicedoc.io/microlinkhq/mql-cli",
  "version": "1.0.1",
  "bin": {
    "mql": "./bin/index.js"
  },
  "author": {
    "email": "josefrancisco.verdu@gmail.com",
    "name": "Kiko Beats",
    "url": "https://github.com/Kikobeats"
  },
  "contributors": [
    {
      "name": "ImgBotApp",
      "email": "ImgBotHelp@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microlinkhq/mql.git"
  },
  "bugs": {
    "url": "https://github.com/microlinkhq/mql/issues"
  },
  "keywords": [
    "api",
    "cli",
    "language",
    "microlink",
    "mql",
    "query"
  ],
  "dependencies": {
    "@microlink/mql": "~0.3.6",
    "beauty-error": "~1.1.3",
    "chalk": "~2.4.2",
    "clipboardy": "~2.0.0",
    "exists-file": "~3.0.2",
    "gradient-string": "~1.2.0",
    "indent-string": "~4.0.0",
    "jsome": "~2.5.0",
    "lodash": "~4.17.11",
    "object-sizeof": "~1.3.0",
    "pretty-bytes": "~5.2.0",
    "pretty-ms": "~5.0.0",
    "terminal-link": "~1.3.0",
    "time-span": "~3.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "ci-publish": "latest",
    "conventional-github-releaser": "latest",
    "eachdir": "latest",
    "finepack": "latest",
    "git-authors-cli": "latest",
    "git-dirty": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "meow": "latest",
    "npm-check-updates": "latest",
    "prettier-standard": "latest",
    "standard": "latest",
    "standard-version": "latest",
    "update-notifier": "latest"
  },
  "engines": {
    "node": ">= 8"
  },
  "files": [
    "bin",
    "examples"
  ],
  "scripts": {
    "clean": "rm -rf node_modules",
    "lint": "standard",
    "postrelease": "npm run release:tags && npm run release:github && ci-publish",
    "precommit": "lint-staged",
    "prerelease": "npm run update:check && git-authors-cli",
    "pretest": "npm run lint",
    "release": "git add package.json && standard-version -a",
    "release:github": "conventional-github-releaser -p angular",
    "release:tags": "git push --follow-tags origin HEAD:master",
    "test": "exit 0",
    "update": "ncu -u",
    "update:check": "ncu -- --error-level 2"
  },
  "preferGlobal": true,
  "license": "MIT",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "package.json": [
        "finepack",
        "git add"
      ],
      "*.js,!*.min.js": [
        "prettier-standard",
        "git add"
      ]
    }
  },
  "standard-version": {
    "scripts": {
      "prechangelog": "git-authors-cli"
    }
  }
}
